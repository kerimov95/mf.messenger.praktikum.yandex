{
  "compilerOptions": {
    "target": "es2017",
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "alwaysStrict": true,
    "sourceMap": true,
    "strictNullChecks": true,
    "outDir": "./static/www",
    "experimentalDecorators": true,
    "paths": {
      "handlebars": [
        "./node_modules/handlebars/dist/handlebars.js"
      ]
    },
    "lib": [
      "es2017",
      "dom"
    ]
  }
}